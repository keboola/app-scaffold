{
    "author": "Keboola",
    "description": "CRM & MRR-Salesforce (+SF Writer)",
    "inputs": [
        {
            "id": "htnsExSalesforceMRR",
            "componentId": "htns.ex-salesforce",
            "schema": {
                "type": "object",
                "required": [
                    "loginname",
                    "#password",
                    "#securitytoken"
                ],
                "properties": {
                    "loginname": {
                        "title": "Login Name",
                        "type": "string"
                    },
                    "#password": {
                        "title": "Password",
                        "type": "password"
                    },
                    "#securitytoken": {
                        "title": "Security Token",
                        "type": "password"
                    },
                    "sandbox": {
                        "type": "boolean",
                        "default" : false
                    }
                }
            }
        },
        {
            "id": "keboolaWrDbSnowflakeLooker",
            "componentId": "keboola.wr-db-snowflake",
            "schema": {
                "type": "object",
                "required": [
                    "db"
                ],
                "properties": {
                    "db": {
                        "type": "object",
                        "required": [
                            "host",
                            "user",
                            "#password",
                            "database",
                            "schema",
                            "warehouse"
                        ],
                        "properties": {
                            "host": {
                                "title": "Host Name",
                                "type": "string"
                            },
                            "user": {
                                "title": "Username",
                                "type": "string"
                            },
                            "schema": {
                                "title": "Schema",
                                "type": "string"
                            },
                            "database": {
                                "title": "Database",
                                "type": "string"
                            },
                            "#password": {
                                "title": "Password",
                                "type": "password"
                            },
                            "warehouse": {
                                "title": "Warehouse",
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    ]
}
