{
    "componentId": "kds-team.ex-paymo",
    "payload": {
        "name": "Paymo",
        "configuration": {
            "parameters": {
                "api": {
                    "baseUrl": "https:\/\/app.paymoapp.com\/api\/",
                    "pagination": {
                        "method": "multiple",
                        "scrollers": {
                            "pagenum_scroller": {
                                "method": "pagenum",
                                "limit": 100,
                                "limitParam": "page_size",
                                "pageParam": "page"
                            }
                        }
                    },
                    "http": {
                        "headers": {
                            "Authorization": {
                                "function": "concat",
                                "args": [
                                    "Basic ",
                                    {
                                        "function": "base64_encode",
                                        "args": [
                                            {
                                                "function": "concat",
                                                "args": [
                                                    {
                                                        "attr": "#username"
                                                    },
                                                    ":no_password_required"
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                },
                "config": {
                    "days": "-60 days",
                    "incrementalOutput": true,
                    "jobs": [
                        {
                            "endpoint": "clientcontacts",
                            "dataType": "client_contact",
                            "dataField": "clientcontacts"
                        },
                        {
                            "endpoint": "clients",
                            "dataType": "client",
                            "dataField": "clients"
                        },
                        {
                            "endpoint": "projects",
                            "dataType": "project",
                            "dataField": "projects"
                        },
                        {
                            "endpoint": "projects",
                            "dataType": "project",
                            "dataField": "projects"
                        },
                        {
                            "endpoint": "tasks",
                            "dataType": "task",
                            "dataField": "tasks",
                            "scroller": "pagenum_scroller"
                        },
                        {
                            "endpoint": "tasklists",
                            "dataType": "tasklist",
                            "dataField": "tasklists"
                        },
                        {
                            "endpoint": "entries",
                            "dataType": "entry",
                            "dataField": "entries",
                            "params": {
                                "where": {
                                    "function": "concat",
                                    "args": [
                                        "time_interval in (\"",
                                        {
                                            "function": "date",
                                            "args": [
                                                "Y-m-d",
                                                {
                                                    "function": "strtotime",
                                                    "args": [
                                                        {
                                                            "attr": "days"
                                                        },
                                                        {
                                                            "function": "time"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        "T00:00:00Z\",\"",
                                        {
                                            "function": "date",
                                            "args": [
                                                "Y-m-d"
                                            ]
                                        },
                                        "T23:59:59Z\")"
                                    ]
                                }
                            }
                        },
                        {
                            "endpoint": "users",
                            "dataType": "user",
                            "dataField": "users",
                            "children": [
                                {
                                    "endpoint": "userstasks?where=user_id={id}",
                                    "dataType": "user_task",
                                    "dataField": "userstasks",
                                    "placeholders": {
                                        "id": "id"
                                    }
                                }
                            ]
                        }
                    ],
                    "mappings": {
                        "client_contact": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "clientcontacts_id",
                                    "primaryKey": true
                                }
                            },
                            "client_id": "client_id",
                            "name": "name"
                        },
                        "client": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "client_id",
                                    "primaryKey": true
                                }
                            },
                            "name": "name",
                            "address": "address",
                            "city": "city",
                            "state": "state",
                            "country": "country",
                            "website": "website",
                            "fiscal_information": "fiscal_information",
                            "active": "active"
                        },
                        "project": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "project_id",
                                    "primaryKey": true
                                }
                            },
                            "client_id": "client_id",
                            "name": "name",
                            "code": "code",
                            "description": "description",
                            "price_per_hour": "price_per_hour",
                            "budget_hours": "budget_hours",
                            "billable": "billable",
                            "created_on": "created_on",
                            "billing_type": "billing_type",
                            "active": "active"
                        },
                        "task": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "task_id",
                                    "primaryKey": true
                                }
                            },
                            "project_id": "project_id",
                            "tasklist_id": "tasklist_id",
                            "name": "name",
                            "code": "code",
                            "description": "description",
                            "price_per_hour": "price_per_hour",
                            "estimated_price": "estimated_price",
                            "budget_hours": "budget_hours",
                            "billable": "billable",
                            "created_on": "created_on",
                            "billing_type": "billing_type",
                            "complete": "complete"
                        },
                        "tasklist": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "tasklist_id",
                                    "primaryKey": true
                                }
                            },
                            "project_id": "project_id",
                            "name": "name",
                            "created_on": "created_on"
                        },
                        "entry": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "entry_id",
                                    "primaryKey": true
                                }
                            },
                            "task_id": "task_id",
                            "project_id": "project_id",
                            "user_id": "user_id",
                            "description": "description",
                            "start_time": "start_time",
                            "end_time": "end_time",
                            "date": "date",
                            "created_on": "created_on",
                            "duration": "duration",
                            "billed": "billed"
                        },
                        "user": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "user_id",
                                    "primaryKey": true
                                }
                            },
                            "name": "name",
                            "email": "email",
                            "type": "type",
                            "active": "active",
                            "created_on": "created_on",
                            "timezone": "timezone"
                        },
                        "user_task": {
                            "id": {
                                "type": "column",
                                "mapping": {
                                    "destination": "user_task_id",
                                    "primaryKey": true
                                }
                            },
                            "user_id": "user_id",
                            "task_id": "task_id",
                            "created_on": "created_on"
                        }
                    }
                }
            },
            "processors": {
                "after": [
                    {
                        "definition": {
