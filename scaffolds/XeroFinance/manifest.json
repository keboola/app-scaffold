{
    "name": "Xero Finance to Snowflake",
    "author": "Keboola",
    "description": "Bank account and Invoice analysis from Xero with output of simple data model to Snowflake. Often combined with other scaffolds (such as support) for a more complete view of the customer's activities.",
    "inputs": [
        {
            "id": "vokurkaExXeroXero",
            "componentId": "vokurka.ex-xero",
            "name": "Xero",
            "required": true,
            "schema": {
                "type": "object",
                "required": [
                    "#private_key",
                    "#consumer_secret"
                ],
                "properties": {
                    "#private_key": {
                        "title": "Private key",
                        "type": "string",
                        "format": "password"
                    },
                    "#consumer_secret": {
                        "title": "Consumer secret",
                        "type": "string",
                        "format": "password"
                    }
                }
            }
        },
        {
            "id": "transformationXero",
            "componentId": "transformation",
            "name": "Xero",
            "required": true,
            "schema": null
        },
        {
            "id": "keboolaWrDbSnowflakeSnowflake",
            "componentId": "keboola.wr-db-snowflake",
            "name": "Snowflake",
            "required": true,
            "schema": null
        },
        {
            "id": "orchestrationFinance",
            "componentId": "orchestrator",
            "name": "Finance",
            "required": true,
            "schema": null
        }
    ]
}
