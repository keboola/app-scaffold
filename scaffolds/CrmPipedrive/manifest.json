            "required": true,
            "schema": {
                "type": "object",
                "required": [
                    "config"
                ],
                "properties": {
                    "config": {
                        "type": "object",
                        "title": "Pipedrive account configuration",
                        "required": [
                            "#apiToken",
                            "companyDomain"
                        ],
                        "properties": {
                            "#apiToken": {
                                "title": "API Token",
                                "type": "string",
                                "format": "password",
                                "descrtiption": "Fill in your Pipedrive API Token (https://pipedrive.readme.io/docs/how-to-find-the-api-token)."
                            },
                            "companyDomain": {
                                "title": "Company Domain",
                                "type": "string",
                                "descrtiption": "Fill in your Pipedrive Company Domain (https://pipedrive.readme.io/docs/how-to-get-the-company-domain)."
                            }
                        }
                    }
                }
            }
        },
        {
            "id": "transformationPipedriveCRM",
            "componentId": "transformation",
            "name": "Pipedrive CRM",
            "required": true,
            "schema": null
        },
        {
            "id": "keboolaWrDbSnowflakeSnowflake",
            "componentId": "keboola.wr-db-snowflake",
            "name": "Snowflake",
            "required": true,
            "schema": null
        },
        {
            "id": "orchestrationPipedriveCRM",
            "componentId": "orchestrator",
            "name": "Pipedrive CRM",
            "required": true,
            "schema": null
        }
    ]
