                    "config"
                ],
                "properties": {
                    "config": {
                        "type": "object",
                        "required": [
                            "#apiToken",
                            "companyDomain"
                        ],
                        "properties": {
                            "#apiToken": {
                                "title": "API Token",
                                "type": "string",
                                "format": "password"
                            },
                            "companyDomain": {
                                "title": "Company domain",
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        {
            "id": "transformationPipedriveCRM",
            "componentId": "transformation",
            "name": "Pipedrive CRM",
            "required": true,
            "schema": null
        },
        {
            "id": "keboolaWrDbSnowflakeSnowflake",
            "componentId": "keboola.wr-db-snowflake",
            "name": "Snowflake",
            "required": true,
            "schema": null
        },
        {
            "id": "orchestrationPipedriveCRM",
            "componentId": "orchestrator",
            "name": "Pipedrive CRM",
            "required": true,
            "schema": null
        }
    ]
